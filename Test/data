local fib2 = "(A1 imp (A2))"
local fib3 = fib2.." imp (((A1 imp (A2 imp (A3))))"
local fib4 = fib3.." imp (((A2 imp (A3 imp (A4))))"
local fib5 = fib4.." imp (((A3 imp (A4 imp (A5))))"

Formula{"Fibonacci3", fib3.." imp (A1 imp (A3)))"}
Formula{"Fibonacci5", fib5.." imp (A1 imp (A5)))))"}

Formula{"Valid1", "(A imp (B)) imp ((B imp (C)) imp (B imp (D imp (C))))"}
Formula{"Valid2", "A imp ((A imp (p)) imp ((q imp (q)) imp (p)))"}
Formula{"Valid3", "(B imp ((C imp (A)))) imp ((A imp (B)) imp ((A imp (C)) imp ((A imp (C)))))"}
Formula{"Valid4", "(q imp (p imp (p))) imp (p imp (q imp (p)))"}
Formula{"Valid5", "(A imp (B imp (C))) imp (B imp (A imp (C)))"}
Formula{"Valid6", "(q imp (p)) imp (q imp (p))"}
Formula{"Valid7", "(q imp (p)) imp (p imp (p))"}
Formula{"Valid8", "A imp ((A imp (t)) imp ((t imp (r)) imp ((r imp (p)) imp (p))))"}

-- Chi Formulas:
local chi1 = "C"
local chi2 = "(((D imp (C)) imp (D)) imp (D)) imp (C)"
local chi3 = "(((E imp ("..chi2..")) imp (E)) imp (E)) imp ("..chi2..")"

local alpha1 = "((((A imp ("..chi1..")) imp (A)) imp (A)) imp ("..chi1..")) imp (C)"
Formula{"HermannTest", alpha1}

